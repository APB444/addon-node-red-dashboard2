{
  "name": "Node-Red Dashboard",
  "version": "dev1",
  "slug": "node-red-dashboard",
  "description": "Proxy That uses Home Assistant Ingress and Nginx to show the node red dashboard",
  "url": "https://github.com/regevbr/addon-node-red-dashboard",
  "ingress": true,
  "ingress_port": 0,
  "panel_icon": "mdi:clock",
  "startup": "application",
  "panel_admin": "false",
  "init": false,
  "homeassistant_api": false,
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "boot": "auto",
  "hassio_api": true,
  "host_network": true,
  "auto_uart": true,
  "privileged": ["SYS_RAWIO"],
  "apparmor": false
}
